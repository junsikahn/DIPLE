$(function(){
  $('#online_lecture_form').html("<%= escape_javascript(render 'new') %>").modal('show');

  var start_date = $('#match_current_period').data('start');
  var end_date = $('#match_current_period').data('end');
  $('#online_lecture_form').find('form').attr('action', $('#online_lecture_form').find('form').attr('action') + '?start_date=' + start_date + '&end_date=' + end_date);

  var dropdownFields = $('#online_lecture_form').find('select');
  dropdownFields.dropdown({
    allowReselection: true
  });
});
